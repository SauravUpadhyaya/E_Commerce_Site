@model WebApplication3.Models.SpecialType
@{
    ViewData["Title"] = "Details";
}

<h2 class="text-info">Details</h2>




<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Details" asp-controller="SpecialTag" method="post" role="form">

            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group" style="display: flex">

                <label asp-for="SpecialTagType" style="width: 190px;" class="control-label"></label>
                <input asp-for="SpecialTagType" class="form-control" readonly="readonly" />

            </div>
            <span asp-validation-for="SpecialTagType" class="text-danger"></span>

            <div class="form-group">
                <a asp-action="Edit" asp-route-id="@Model.Id" class="btn  btn-primary">Edit</a>
                <a asp-action="Index" class="btn btn-success">Back to List</a>
                @*<input type="submit" class="btn btn-success" value="Back to List" />*@
            </div>

        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

