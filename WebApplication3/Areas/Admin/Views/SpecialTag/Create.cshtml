@model WebApplication3.Models.SpecialType

<h2 class="text-info">Special Tag List</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" asp-controller="SpecialTag" method="post" role="form">

                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group" style="display: flex">
                    
                    <label asp-for="SpecialTagType"  style="width: 190px;" class="control-label"></label>
                    <input asp-for="SpecialTagType" class="form-control" />

                </div>

                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-default" />
                    <a asp-action="Index" class="btn btn-success">Back to List</a>
                </div>
            </form>
</div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}